{
  "name": "fisherbackflows",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -H 0.0.0.0 -p 3010",
    "dev-turbo": "next dev -H 0.0.0.0 -p 3010 --turbopack",
    "build": "next build",
    "build-turbo": "next build --turbopack",
    "start": "next start -H 0.0.0.0 -p 3010",
