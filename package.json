    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest --selectProjects unit integration",
    "test:unit": "jest --selectProjects unit --runInBand",
    "test:integration": "jest --selectProjects integration --runInBand --passWithNoTests",
    "test:e2e": "playwright test",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:smoke": "jest tests/smoke --testTimeout=30000",
    "test:health": "node scripts/health-check.js",
    "test:build": "npm run build && npm run test:unit",
    "db:setup:test": "node scripts/setup-test-db.js",
    "setup": "node scripts/verify-setup.js",
    "verify": "node scripts/verify-setup.js",
    "ai:install-schema": "node scripts/install-ai-schema.js",
    "ai:test": "node scripts/test-ai-features.js",
    "ai:verify-data": "node scripts/verify-real-data.js",
    "ai:setup": "npm run ai:install-schema && npm run ai:test && npm run ai:verify-data"
  },
